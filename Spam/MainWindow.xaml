<Window x:Class="Spam.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525" Closed="Window_Closed">
    <Grid>
        <Button Content="Add" Height="23" HorizontalAlignment="Left" Margin="243,12,0,0" Name="btnAddMail" VerticalAlignment="Top" Width="75" Click="btnAddMail_Click" />
        <RichTextBox Height="287" HorizontalAlignment="Left" Margin="12,12,0,0" Name="txtText" VerticalAlignment="Top" Width="225" />
        <Label Content="Label" Height="96" HorizontalAlignment="Left" Margin="339,12,0,0" Name="label1" VerticalAlignment="Top" />
        <Button Content="Import" Height="23" HorizontalAlignment="Left" Margin="243,41,0,0" Name="btnImport" VerticalAlignment="Top" Width="75" Click="btnImport_Click" />
        <Button Content="Clear Box" Height="23" HorizontalAlignment="Left" Margin="243,70,0,0" Name="btnClearBox" VerticalAlignment="Top" Width="75" Click="btnClearBox_Click" />
        <Button Content="Count" Height="23" HorizontalAlignment="Left" Margin="243,99,0,0" Name="btnCount" VerticalAlignment="Top" Width="75" Click="btnCount_Click" />
        <CheckBox Content="Top Most" Height="16" HorizontalAlignment="Left" Margin="243,128,0,0" Name="chkTop_Most" VerticalAlignment="Top" Checked="chkTop_Most_Checked" Unchecked="chkTop_Most_Unchecked" />
    </Grid>
</Window>
